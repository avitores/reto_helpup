<%- if controller_name != 'sessions' %>
  <%= button_to "Conectar", new_session_path(resource_name), class:'btn btn-lg btn-warning navbar-btn' %><br/>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrarse", new_registration_path(resource_name), class:'btn btn-lg btn-warning navbar-btn' %>
<% end -%>
